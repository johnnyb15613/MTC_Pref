<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="?attr/primaryBgColor">

    <TextView
        android:id="@+id/nav_row_sub_header"
        android:paddingLeft="@dimen/nav_item_paddingLeftRight"
        android:paddingTop="@dimen/nav_item_paddingTopBottom"
        android:paddingBottom="@dimen/nav_item_paddingTopBottom"
